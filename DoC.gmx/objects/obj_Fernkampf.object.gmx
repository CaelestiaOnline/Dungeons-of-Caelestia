                    bullet2.image_angle = 180;
            }
            if(countdown_erzeugen &lt; 0)
            {
                countdown_erzeugen = attack_speed;
            }
            countdown_erzeugen--;
        }
    }
}
//Countdown zurücksetzen
if(keyboard_check_released(vk_space))
