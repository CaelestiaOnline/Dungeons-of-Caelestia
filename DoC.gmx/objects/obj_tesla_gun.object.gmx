if(!admin.playing) exit;

if(hit_list != noone) {
    var n = 0;
    while(n &lt; ds_list_size(hit_list)) {
        if(instance_exists(hit_list[| n])) {
            draw_set_alpha(0.3);
            draw_set_colour(c_aqua);
            draw_line_width(hit_x, obj_player.y, hit_list[| n].x, hit_list[| n].y, 5);
            draw_set_alpha(1);
        }
        n++;
    }
    ds_list_destroy(hit_list);
    hit_list = noone;
}

if(draw) {
    repeat(irandom(5)+5) {
        draw_set_alpha(0.3);
        draw_set_colour(c_aqua);
        draw_line_width(obj_player.x +(32*obj_player.image_xscale) + irandom_range(-15, 15), obj_player.y, obj_player.x +(32*obj_player.image_xscale) + irandom(150) * obj_player.image_xscale, obj_player.y + random_range(-55, 55), 5);
        draw_set_alpha(1);
    }
    draw = false;
}
