scr_prim_ability();

if(confirmed) {
    real_damage = scr_get_item_damage(random(damage), damage_crit);
    
    confirmed = false;
    
    draw = true;
    
    if(obj_player.attack_axis = 1) hit_x = obj_player.x + sprite_get_width(obj_player.sprite_index)/2 - 32;
    if(obj_player.attack_axis = -1) hit_x = obj_player.x - sprite_get_width(obj_player.sprite_index)/2 - sprite_get_width(spr_hitbox)*image_xscale + 32;
    
    //Hitbox
    scr_create_hitbox(
        hit_x,
        obj_player.y,
        damage,
        is_crit
    );
    
}

if(hit_list != noone) {
    var n = 0;
    while(n &lt; ds_list_size(hit_list)) {
        scr_deal_damage(hit_list[| n], real_damage);
        n++;
    }
}
