            <string>///Stats
//Level
var collected_xp = death_stats.collected_xp;
var collected_xp_copy = collected_xp;
level_old = progress.level;

while(collected_xp_copy &gt; 0) {
    if(collected_xp_copy &gt;= scr_get_level_up_xp()) {
        collected_xp_copy -= scr_get_level_up_xp();
        progress.level++;
    } else {
        progress.level += collected_xp_copy / scr_get_level_up_xp();
        break;
    }
}

new_level = progress.level - level_old;

level_up = false;
if(floor(level_old) &lt; floor(progress.level)) {
    level_up = true;
}

new_skillpoints = int64(floor(progress.level) - floor(level_old));

progress.skillpoints += new_skillpoints;

new_hub_money = stats.collected_run_money/64;

progress.hub_money += new_hub_money;


//Save Data
scr_write_progress();
scr_write_stats();
scr_read_stats();
</string>
